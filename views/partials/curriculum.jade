div(ng-controller='CurriculumCtrl')
  ul.row
    li(ng-repeat='entry in curriculum', ng-bind='entry')
    .row
      h1 {{experince}}
      .col-md-6(ng-repeat='exp in experience')
        h1
          small Secondary text
        |               {{exp.}}
      .col-md-6
        | ahth
    .row.curriculum-skills(ng-repeat='cat in skills')
      h4.col-sm-12(ng-bind='cat.name')
      

      .col-sm-3(ng-repeat='skill in cat.items')
        span(ng-bind='skill.name')

        .progress.curriculum-points
          .progress-bar.progress-bar-success(role='progressbar', aria-valuenow='{{ skill.point }}', aria-valuemin='0', aria-valuemax='3', ng-style='{width : percentia(skill.point) }')
